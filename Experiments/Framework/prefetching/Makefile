all: prefetching prefetching2 prefetching3 prefetching4 prefetching5


prefetching: prefetching.cpp ../../../DataStructures ../../../DataStructures
#	g++ -std=c++11 ../../../DataStructures/StandardArray.cpp ../../../DataStructures/DynamicArray.cpp \
	../../../DataStructures/LinkedList.cpp ../../../DataStructures/CountedBtree.cpp insert.cpp -o insert \
	-L/usr/include/mysql -lmysqlclient -I/usr/include/mysql
	g++ -std=c++11 -Wall -g ../../../DataStructures/DynamicArray.cpp prefetching.cpp \
    -I/usr/include/cppconn -o prefetching -pthread -L/usr/lib -lmysqlcppconn


